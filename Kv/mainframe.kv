#:import NavigationLayout kivymd.navigationdrawer.NavigationLayout
#:import MDSeparator kivymd.cards.MDSeparator
#:import images_path kivymd.images_path
#:import MDFillRoundFlatButton kivymd.button.MDFillRoundFlatButton
#:import MDLabel kivymd.label.MDLabel
#:import MDTextFieldRound kivymd.textfields.MDTextFieldRound
#:import MDToolbar kivymd.toolbar.MDToolbar

<MainFrame>:
	FloatLayout:
        canvas:
            Rectangle:
                size: self.size
                pos: self.pos
                source: 'Image/account-background.jpeg'

        MDToolbar:
        	id: toolbar
	        title: app.title
	        md_bg_color: app.theme_cls.primary_color
	        background_palette: 'Primary'
	        elevation: 10
	        right_action_items: [['dots-vertical', lambda x: None]]
	        left_action_items: [['menu', lambda x: '']]
	        pos_hint: {'top': 1}

		NavigationLayout:

			MDNavigationDrawer:
				id: nav
				NavigationDrawerIconButton:
					text: "Setting"
					id: Setting
				NavigationDrawerIconButton:
					text: "Log Out"
					id: Log_out
					on_release: app.change_screen("login")