#:import NavigationLayout kivymd.navigationdrawer.NavigationLayout
#:import MDSeparator kivymd.cards.MDSeparator
#:import Window kivy.core.window.Window
#:import MDLabel kivymd.label.MDLabel
#:import MDFillRoundFlatButton kivymd.button.MDFillRoundFlatButton
#:import MDCustomRoundIconButton kivymd.button.MDCustomRoundIconButton


<MainFrame>:
	FloatLayout:
        canvas:
            Rectangle:
                size: self.size
                pos: self.pos
                source: 'Image/account-background.jpeg'

		NavigationLayout:
			Button:
		    	size_hint: (0.13, 0.07)
		    	center_x: (root.width/2)-50
		    	top: (root.top/2)-15
		    	size: 90,35
		    	pos: 350, 200
		    	font_size: 18
		    	text: "Forgot"
		        on_release: root.log_in()
		        pos_hint: {'center_x': 0.43, 'center_y': 0.4}
			MDNavigationDrawer:
				NavigationDrawerIconButton:
					text: "Setting"
					id: Setting
				NavigationDrawerIconButton:
					text: "Log Out"
					id: Log_out